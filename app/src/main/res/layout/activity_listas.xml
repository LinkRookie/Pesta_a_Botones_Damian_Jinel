<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#FAFAFA"
    android:backgroundTint="#FFFFFF"
    tools:context=".Listas">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="90dp"
        android:layout_marginTop="10dp"
        android:background="#B1AEAE"
        android:backgroundTint="#FBFBFB"
        android:orientation="horizontal"
        app:layout_constraintTop_toBottomOf="@+id/linearLayout7">

        <ImageView
            android:id="@+id/imageView17"
            android:layout_width="142dp"
            android:layout_height="75dp"
            android:layout_marginStart="30dp"
            android:layout_marginTop="10dp"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/imageView12"
            app:srcCompat="@drawable/postre2" />

        <TextView
            android:id="@+id/textView16"
            android:layout_width="243dp"
            android:layout_height="45dp"
            android:layout_weight="1"
            android:background="#B1AEAE"
            android:backgroundTint="#00B1AEAE"
            android:text="Tiramisu                                          $6.000"
            android:textColor="#6A2C2C" />
    </LinearLayout>

    <LinearLayout
        android:id="@+id/linearLayout10"
        android:layout_width="415dp"
        android:layout_height="100dp"
        android:layout_marginTop="10dp"
        android:background="#B1AEAE"
        android:backgroundTint="#FBFBFB"
        android:orientation="horizontal"
        app:layout_constraintTop_toBottomOf="@+id/linearLayout6">

        <ImageView
            android:id="@+id/imageView12"
            android:layout_width="142dp"
            android:layout_height="90dp"
            android:layout_marginStart="30dp"
            android:layout_marginTop="10dp"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:srcCompat="@drawable/postre1" />

        <TextView
            android:id="@+id/textView12"
            android:layout_width="239dp"
            android:layout_height="49dp"
            android:background="#B1AEAE"
            android:backgroundTint="#00BFBFBF"
            android:text="Postre de Galleta                                             $3.500"
            android:textColor="#6A2C2C" />

    </LinearLayout>

    <LinearLayout
        android:id="@+id/linearLayout9"
        android:layout_width="match_parent"
        android:layout_height="98dp"
        android:layout_marginTop="10dp"
        android:background="#B1AEAE"
        android:backgroundTint="#FBFBFB"
        android:orientation="horizontal"
        app:layout_constraintTop_toBottomOf="@+id/linearLayout8">

        <ImageView
            android:id="@+id/imageView19"
            android:layout_width="141dp"
            android:layout_height="82dp"
            android:layout_marginStart="30dp"
            android:layout_marginTop="10dp"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/imageView18"
            app:srcCompat="@drawable/postre4" />

        <TextView
            android:id="@+id/textView14"
            android:layout_width="240dp"
            android:layout_height="47dp"
            android:layout_weight="1"
            android:background="#B1AEAE"
            android:backgroundTint="#00FFFFFF"
            android:text="Postres de Tres leches                                          $3.500"
            android:textColor="#6A2C2C" />
    </LinearLayout>

    <LinearLayout
        android:id="@+id/linearLayout7"
        android:layout_width="match_parent"
        android:layout_height="104dp"
        android:layout_marginTop="10dp"
        android:background="#B1AEAE"
        android:backgroundTint="#E6E6E6"
        android:orientation="horizontal"
        app:layout_constraintTop_toBottomOf="@+id/linearLayout9">

        <ImageView
            android:id="@+id/imageView18"
            android:layout_width="147dp"
            android:layout_height="100dp"
            android:layout_marginStart="30dp"
            android:layout_marginTop="10dp"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/imageView17"
            app:srcCompat="@drawable/postre3" />

        <TextView
            android:id="@+id/textView15"
            android:layout_width="245dp"
            android:layout_height="57dp"
            android:layout_weight="1"
            android:background="#B1AEAE"
            android:backgroundTint="#00FFFFFF"
            android:text="Postre  Tiramisu                                           $4.500"
            android:textColor="#6A2C2C" />
    </LinearLayout>

    <LinearLayout
        android:id="@+id/linearLayout8"
        android:layout_width="match_parent"
        android:layout_height="104dp"
        android:layout_marginTop="10dp"
        android:background="#B1AEAE"
        android:backgroundTint="#E6E6E6"
        android:orientation="horizontal"
        app:layout_constraintTop_toBottomOf="@+id/linearLayout10">

        <ImageView
            android:id="@+id/imageView20"
            android:layout_width="144dp"
            android:layout_height="90dp"
            android:layout_marginStart="30dp"
            android:layout_marginTop="10dp"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/imageView19"
            app:srcCompat="@drawable/postre5" />

        <TextView
            android:id="@+id/textView13"
            android:layout_width="240dp"
            android:layout_height="65dp"
            android:layout_weight="1"
            android:background="#B1AEAE"
            android:backgroundTint="#00B1AEAE"
            android:text="Patel de Galletas maria con chocolates                                         $5.500"
            android:textColor="#6A2C2C" />
    </LinearLayout>

    <LinearLayout
        android:id="@+id/linearLayout6"
        android:layout_width="412dp"
        android:layout_height="121dp"
        android:orientation="horizontal"
        tools:layout_editor_absoluteX="1dp"
        tools:layout_editor_absoluteY="1dp">

        <ImageView
            android:id="@+id/imageView10"
            android:layout_width="match_parent"
            android:layout_height="114dp"
            android:layout_weight="1"
            app:srcCompat="@drawable/logoa" />
    </LinearLayout>

    <Button
        android:id="@+id/button4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="3dp"
        android:text="Agregar"
        app:layout_constraintBottom_toBottomOf="parent"
        tools:layout_editor_absoluteX="152dp" />

</androidx.constraintlayout.widget.ConstraintLayout>